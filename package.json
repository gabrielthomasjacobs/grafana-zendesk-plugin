{
  "name": "gabrielthomasjacobs-zendesk-datasource",
  "version": "1.1.0",
  "description": "Adds zendesk integration",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Gabrielthomasjacobs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@grafana/e2e": "10.0.1",
    "@grafana/e2e-selectors": "10.0.1",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.6",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.0.1",
    "@grafana/runtime": "10.0.1",
    "@grafana/ui": "10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
